{
    "name": "caesargustav/mauve-php-sdk",
    "description": "A PHP HTTP Client for the Mauve Mailorder API",
    "version": "0.0.2",
    "keywords": [
        "php",
        "client",
        "mauve",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Kleyersburg",
            "email": "tk@wacg.de"
        }
    ],
    "require": {
        "php": "^8.3|^8.4",
        "saloonphp/saloon": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "CaesarGustav\\MauveApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^7.2",
        "laravel/pint": "^1.21",
        "pestphp/pest": "^3.7"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
